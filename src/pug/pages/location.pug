
extends ../layout/main
block variable
  -
    var title_page = 'location'
    var id_page = 'id-page-location'
    var class_page = 'class-page-location'
    var headerCustomClass = 'type1' 
    var breadcrumbsSubitem = [
      {
        label: 'About',
        link: pagesLink[0].about
      }
    ]
  
block meta
  title= title_page
  meta(name='description', content='')
block head
    link(rel='stylesheet', href="./assets/styles/main.min.css")
block scripts
